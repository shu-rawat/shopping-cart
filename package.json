{
  "name": "code-exercise",
  "version": "1.0.0",
  "description": "This is a shopping cart coding exercise",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run server",
    "build": "webpack -p && npm run move",
    "move":"cpx \"src/**/*.{html,png,jpg,json}\" dist && cpx \"server/**/*.{html,png,jpg,json}\" dist && cpx \"static/**/*.{png,jpg,svg}\" dist/static && cpx \"index.html\" dist",
    "server": "live-server ./dist --port=3001 --cors"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-loader": "^8.0.5",
    "core-js": "^2.6.5",
    "css-loader": "^2.1.1",
    "express-handlebars": "^3.0.2",
    "handlebars-loader": "^1.7.1",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1"
  },
  "devDependencies": {
    "cpx": "^1.5.0"
  }
}
