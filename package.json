{
  "name": "code-exercise",
  "version": "1.0.0",
  "description": "This is a shopping cart coding exercise",
  "main": "index.js",
  "scripts": {
    "deploy": "npm run build && npm run server",
    "dev": "npm run build-dev | npm run server",
    "build": "webpack -p && npm run move",
    "build-dev": "webpack -p -w | npm run move-dev",
    "move": "cpx \"src/**/*.{html,png,jpg,json}\" dist && cpx \"server/**/*.{html,png,jpg,json}\" dist && cpx \"static/**/*.{png,jpg,svg}\" dist/static && cpx \"index.html\" dist",
    "move-dev": "cpx \"src/**/*.{html,png,jpg,json}\" dist --watch | cpx \"server/**/*.{html,png,jpg,json}\" dist --watch | cpx \"static/**/*.{png,jpg,svg}\" dist/static --watch | cpx \"index.html\" dist --watch",
    "server": "live-server ./dist --port=3001 --cors",
    "unit": "karma start karma.conf.js ",
    "webpack": "webpack -p -w",
    "test": "npm run unit"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.5",
    "core-js": "^2.6.5",
    "css-loader": "^2.1.1",
    "express-handlebars": "^3.0.2",
    "handlebars-loader": "^1.7.1",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.2",
    "karma-jasmine": "^2.0.1",
    "karma-webpack": "^3.0.5",
    "live-server": "^1.2.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "^4.1.0",
    "postcss-loader": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-lint-webpack": "^1.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "whatwg-fetch": "^3.0.0"
  },
  "devDependencies": {
    "cpx": "^1.5.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
